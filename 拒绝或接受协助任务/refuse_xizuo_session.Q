[Script]
//这个脚本用来处理协助任务的情况
y_Or_n = 0//接受任务设置为1，拒绝任务设置为0
If y_Or_n = 0 Then
    pic_find = "Attachment:\拒绝协助.bmp"
Else 
    pic_find = "Attachment:\接受协助.bmp"
End If
While True
    //每3秒检查是否出现协助任务
    Delay 3000
    FindPic 858,428,936,612,pic_find,0.9,intX1,intY1
    //以下是条件判断；如果返回的坐标大于0，那么就说明找到了。
    If intX1 > 0 And intY1 > 0 And y_Or_n = 0 Then //点击拒绝协助
        Delay 1000
        MoveTo 898, 566
        Delay 500
        LeftClick 1
    End If
    If intX1 > 0 And intY1 > 0 And y_Or_n = 1 Then //点击接受协助
        Delay 1000
        MoveTo 896, 471
        Delay 500
        LeftClick 1
    End If
Wend
